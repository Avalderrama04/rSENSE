<div class="vis-ctrl-subblock">
  <h4 class="vis-ctrl-subtitle">Zoom</h4>
  <div>
    <select id="zoom-axis-list" class="form-control">
      {{#each axes}}
        <option value="{{this}}">{{this}}</option>
      {{/each}}
    </select>
    <div class="btn-group btn-group-justified">
      <div class="btn-group">
        <button id="zoom-out-btn"
                class="zoom-btn btn btn-default">
          Out
        </button>
      </div>
      <div class="btn-group">
        <button id="zoom-reset-btn"
                class="zoom-btn btn btn-default">
          Fit
        </button>
      </div>
    </div>
  </div>
</div>
<div class="vis-ctrl-subblock">
  <h4 class="vis-ctrl-subtitle">Display Mode</h4>
  <div>
    {{#each modes}}
      <div class="radio">
        <input id="vis-mode-{{mode}}" type="radio" name="mode" value="{{mode}}"/>
        <label for="vis-mode-{{mode}}">{{text}}</label>
      </div>
    {{/each}}
  </div>
</div>
<div class="vis-ctrl-subblock">
  <h4 class="vis-ctrl-subtitle">Other</h4>
  <div>
    <div class="checkbox">
      <input id="ckbx-tooltips" type="checkbox"/>
      <label for="ckbx-tooltips">Detailed Tooltips</label>
    </div>
    <div class="checkbox">
      <input id="ckbx-fulldetail" type="checkbox"/>
      <label for="ckbx-fulldetail">Show All Data</label>
    </div>
    {{#if logSafe}}
      <div class="checkbox">
        <input id="ckbx-log-y" type="checkbox"/>
        <label for="ckbx-log-y">Logarithmic Y Axis</label>
      </div>
    {{/if}}
    {{#if elapsedTime}}
      <div class="centered">
        <button id="elapsed-time-btn" class="save-btn btn btn-default btn-sm">
          Generate Elapsed Time
        </button>
      </div>
    {{/if}}
  </div>
</div>
